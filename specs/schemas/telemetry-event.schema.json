{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Kojibox Telemetry Event",
  "type": "object",
  "required": ["event", "ts", "appVersion", "os", "payload"],
  "properties": {
    "event": {"type": "string"},
    "ts": {"type": "string", "format": "date-time"},
    "appVersion": {"type": "string"},
    "os": {"type": "string"},
    "payload": {"type": "object"}
  }
}
