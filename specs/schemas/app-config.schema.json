{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Kojibox App Config",
  "type": "object",
  "required": ["installPath", "updateChannel", "telemetryOptIn", "updateFeedUrl", "updatePublicKeys"],
  "properties": {
    "installPath": {"type": "string"},
    "updateChannel": {"type": "string", "enum": ["stable", "beta"]},
    "telemetryOptIn": {"type": "boolean"},
    "updateFeedUrl": {"type": "string"},
    "updatePublicKeys": {
      "type": "array",
      "items": {"type": "string"}
    }
  }
}
